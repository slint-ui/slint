    "astro": "5.12.0",
    "astro-embed": "0.9.0",
    "playwright-ctrf-json-reporter": "0.0.21",
    "rehype-external-links": "3.0.0",
    "sharp": "0.34.3",
    "starlight-links-validator": "0.17.0",
    "starlight-sidebar-topics": "0.6.0",
    "typescript": "5.8.3",
    "@slint/common-files": "workspace:*"
