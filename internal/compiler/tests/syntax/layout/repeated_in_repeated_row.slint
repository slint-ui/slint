// Issue #10670

export component Demo {
    GridLayout {
        for x in 10: Row {
            for y in 10: Text {
                text: y;
            }
        }
    }


    in property <bool> condition;
    GridLayout {
        if condition: Row {
            for y in 10: Text {
                text: y;
            }
        }
    }
}
