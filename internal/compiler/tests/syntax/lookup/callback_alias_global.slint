export global Abc {
    callback foobar;
}

component Wrapper {
    callback foo <=> Abc.foobar;
//  ^warning{Aliases to global callback are deprecated}
}

export component E inherits Window {
    Wrapper {
        foo => { debug("hello"); }
//      ^error{Can't assign a local callback handler to an alias to a global callback}
    }
}