export interface Calculator {
    pure function add(a: int, b: int) -> int;
    function format(value: int) -> string;
    function reset();  // void functions don't specify a return type

    // Mix with existing features
    in-out property <int> value;
    callback notify();
}

export interface FunctionWithBody {
    pure function sum(a: int, b: int) -> int {
//                                           >error{Interfaces may only contain function declarations}
        return a + b;
    }
//  <error{Interfaces may only contain function declarations}
}
