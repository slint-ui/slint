export X := Rectangle {
    edit := TextInput { }
    TouchArea {
        clicked => {
            (edit.focus)();
//           ^error{'edit.focus' must be called. Did you forgot the '\(\)'\?}
            edit.focus;
//          ^error{'edit.focus' must be called. Did you forgot the '\(\)'\?}
        }
    }
    x: edit.focus;
//     ^error{Cannot convert function\(element ref\) -> void to length}
//     ^^error{'edit.focus' must be called. Did you forgot the '\(\)'\?}
//          ^^^warning{Call of impure function}
}
