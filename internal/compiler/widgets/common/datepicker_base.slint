export struct Date {
    year: int,
    month: int,
    day: int,
}

export component DatePickerBase {
    in property <Date> date;

    property <Date> current-date;

    pure public function ok-enabled() -> bool {
        true
    }

    public function get-current-date() -> Date {
        root.current-date
    }
}
