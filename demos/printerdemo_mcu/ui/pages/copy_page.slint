import { DemoPalette, Label } from "../common.slint";
import { ComboBox, CheckBox, SpinBox, PushButton, Page } from "../components/components.slint";
import { PrinterQueue } from "./printer_queue.slint";

export component CopyPage inherits Page {
    header: "Copy";

    VerticalLayout {
        padding: 16px;
        spacing: 16px;

        SpinBox {
            label: "Copies";
            value: 1;
            minimum: 1;
        }

        PushButton {
            label: "Continue";
            clicked => {
                PrinterQueue.start-job("Copy");
            }
        }
    }

}
