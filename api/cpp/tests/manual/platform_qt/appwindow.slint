import {Button, AboutSlint} from "std-widgets.slint";

export component App inherits Window {
    preferred-width: 800px;
    preferred-height: 600px;
    property <int> count;

    VerticalLayout {
        AboutSlint {  }
        Button {
            text: "Hello";
            clicked => { count += 1; }
        }
        Text { text: count; }
        Rectangle {  }
    }
}
