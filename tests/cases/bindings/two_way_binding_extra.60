TestCase := Rectangle {
    width: 100phx;
    height: 100phx;

    property<length> xx: 50phx;
    property<length> ww <=> first.width;

    first := TouchArea {
        x <=> parent.xx;
        width: 50phx;
        enabled <=> second.enabled;
        height: 15phx;
        clicked => { result += 40; }
    }

    second := TouchArea {
        x <=> parent.xx;
        width <=> root.ww;
        height <=> first.height;
        clicked => { result += 300; }
        y: 50phx;
    }

    property <int> result: 5;
}



