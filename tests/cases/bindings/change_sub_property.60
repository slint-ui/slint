SubElem := Rectangle {
    property <int> value: sub.value;
    callback change();
    change => { sub.value += 44; }
    sub := Rectangle {
        property <int> value: 55;
    }
}

TestCase := Rectangle {
    callback change();
    change => { sub.change() }
    property <int> toplevel: sub.value + 1;
    sub := SubElem { }
}


