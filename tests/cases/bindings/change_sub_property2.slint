SubElem := Rectangle {
    property <int> value: 55;
    callback change();
    change => { value += 44; }
}

TestCase := Rectangle {
    callback change();
    change => { sub.change(); sub2.change(); }
    property <int> toplevel: sub.value + sub2.value;
    sub := SubElem { }
    sub2 := SubElem { value : 1; }
}


