TestCase := Rectangle {
    width: 800phx;
    height: 800phx;

    Path {
        commands: "M 400 300 A 50 50 0 1 1 550 300 C 550 400 450 450 250 450";
        stroke_width: 2px;
        stroke: black;
    }

    PathLayout {

        commands: "M 400 300 A 50 50 0 1 1 550 300 C 550 400 450 450 250 450";

        Rectangle {
            width: 20phx;
            height: 20phx;
            background: green;
        }

        for t in [
            "Hello",
            "World",
            "!"
        ] : Text {
            color: black;
            text: t;
        }

        Rectangle {
            width: 20phx;
            height: 20phx;
            background: yellow;
        }


    }
}
