export TestCase := Window {
    VerticalLayout {
        padding: 7px;
        Flickable {
            v:= VerticalLayout {
                padding: 0;
                spacing: 0;
                for i in 6: Text {
                    height: 100px;
                    width: 100px;
                    text: "Test";
                }
            }
        }
    }

    property <bool> test: root.min_width == 7px*2 && root.min_height == 7px*2 && root.preferred_width == 114px && root.preferred_height == 14px + 600px;
}

