TestCase := Window {
    width: 300phx;
    height: 300phx;

    property<[int]> xs: [1, 2, 3];
    property<[int]> ys: [10, 20, 30];

    VerticalLayout {
        padding: 0px;
        spacing: 0px;
        for x_[i] in xs: HorizontalLayout {
            padding: 0px;
            spacing: 0px;
            for y_[j] in ys: Rectangle {
                TouchArea {
                    clicked => {
                        last_clicked = x_ + y_;
                    }
                }
                Text { text: (x_ + y_);  }
            }
        }
    }

    property<int> last_clicked;
}
