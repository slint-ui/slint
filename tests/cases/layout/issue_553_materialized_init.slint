// Test the propagation of maximum and minimum size through nested grid layouts

TestCase := Rectangle {
    min-width: 500px;
    max-width: min-width;

    property xxx <=> preferred-height;
    HorizontalLayout {
        Rectangle { height: 88px; }
    }

    property <bool> test: min-width == 500px && max-width == 500px && xxx == 88px;
}

