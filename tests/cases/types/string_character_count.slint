export component TestCase  {
    property<string> empty;
    property<string> hello: "hello";
    property<string> hiragana: "ã‚ã„ã†ãˆãŠ";
    property<string> surrogate-pair: "ğŸ˜Šğ©¸½";
    property<string> variation-selectors: "ğŸ‘ğŸ¿";
