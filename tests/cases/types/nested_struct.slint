// This test case verifies that we do emit the code for the `Nested` struct,
// when it's only indirectly referenced through the `Item` struct.

struct Nested := {
    ok: bool
}

struct Item := {
    nested: Nested
}

export TestCase := Rectangle {
    property <Item> fob;
}
