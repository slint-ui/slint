// Test that structs work if they are only referenced by functions

struct Foo1 { member: int, }
struct Foo2 { a: Foo1 }
struct Foo3 { b: int }

TestCase := TouchArea {
    // Based on Issue #2655
    public function fn1(foo2: Foo2) -> Foo3 {
      return { b: foo2.a.member+1 };
    }
    public function fn2() { fn1({a:{member:456}}); }
}


