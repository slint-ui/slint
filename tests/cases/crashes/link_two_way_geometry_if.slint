export component TestCase {
    tl_layout := HorizontalLayout {
        if true: Rectangle {
            l := Rectangle {
                preferred-width: 320px;
                preferred-height: 240px;
                background: red;
            }
            Rectangle {
                background: blue;
                width <=> l.width;
            }
        }
    }

    out property<bool> test: tl_layout.preferred-width == 320px;
}
