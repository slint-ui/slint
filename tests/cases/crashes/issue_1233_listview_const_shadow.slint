import { ListView } from "std-widgets.slint";

App := Window {
    width: 100px;
    height: 600px;

    ListView {
        for item in [
            {}, {},
        ] : Rectangle {
            drop-shadow-blur: 5px;
            HorizontalLayout {
                Image {
                    source: @image-url("../../../logo/slint-logo-square-light-128x128.png");
                }
            }
        }
    }
}


