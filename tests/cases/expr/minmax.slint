 TestCase := Rectangle {
    property <int> a;
    property <float> t1: max(41, 12, min(100, 12), max(-10000, 0+98.5), -4) + min(a, 0.5);
    property <bool> t2: round(10/4) == 3 && floor(10/4) == 2 && ceil(10/4) == 3;

    r := Rectangle {
        property <int> max: 42;
        property <int> xx: Math.max(1, 2, 3) + max;
    }
    property <bool> test: t2 && r.xx == 42 + 3;
}
