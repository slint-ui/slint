export component Demo inherits Window {
    function b(c:int,d:int) -> bool {
        c == d
    }

    function a() {
        if b(0,1) {
            return;
        }
        b(0,1);
    }

    TouchArea {
        clicked => {
            a();
            bar();
        }
    }


    callback foo() -> int;
    function bar() {
        if foo() == 0 {
            return;
        }
        foo();
    }
}

// gcc-args: -Wno-unused-value

