Container := Rectangle {
    GridLayout {
        padding: 0phx;
        spacing: 0phx;
        Row {
            Rectangle {
                background: green;
            }
            @children
        }
    }
}

MegaContainer := Rectangle {
    Container {
        @children
        width: parent.width;
        height: parent.height;
    }
}

TestCase := MegaContainer {
    width: 300phx;
    height: 200phx;

    rect1 := Rectangle {
        background: black;
    }


    property <bool> rect1_pos_ok: rect1.x == 150phx;
}
