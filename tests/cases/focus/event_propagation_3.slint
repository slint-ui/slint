export component TestCase inherits Window {
    width: 100phx;
    height: 100phx;
    f1 := FocusScope {

        f2 := FocusScope {
            x: 50px;
            y: 50px;
            width: 50px;
            height: 50px;
        }
    }

    out property<bool> correct_focus: !f1.has_focus && f2.has_focus;
}

