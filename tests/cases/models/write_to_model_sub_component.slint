SubComponent := Rectangle {
    property <[int]> m;
    HorizontalLayout {
        for foo in m: Rectangle {
            if foo < 100 : TouchArea {
                clicked => { foo += 8; }
            }
        }
    }
}

TestCase := Rectangle {
    width: 100px;
    height: 100px;
    property <[int]> mod;
    SubComponent {
        m: mod;
    }
}

