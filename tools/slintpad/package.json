{
    "//": "cSpell: ignore codingame dragdrop frsource lumino ",
    "name": "slintpad",
    "version": "1.4.1",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "npm run clean && npx vite build",
        "build:wasm_experimental_lsp": "wasm-pack build --dev --target web ../lsp -- --no-default-features --features backend-winit,renderer-femtovg,preview,experimental",
        "build:wasm_lsp": "wasm-pack build --dev --target web ../lsp -- --no-default-features --features backend-winit,renderer-femtovg,preview",
        "build:wasm_lsp-release": "wasm-pack build --release --target web ../lsp -- --no-default-features --features backend-winit,renderer-femtovg,preview",
        "build:wasm_interpreter": "wasm-pack build --dev --target web ../../api/wasm-interpreter -- --features console_error_panic_hook",
        "build:wasm_interpreter-release": "wasm-pack build --release --target web ../../api/wasm-interpreter -- --features console_error_panic_hook",
        "lint": "eslint src",
        "clean": "rimraf dist dev-dist pkg",
        "start": "npm run clean && npm run build:wasm_lsp && npm run start:vite",
        "start:vite": "vite --open",
        "start:vite_cypress": "vite --port=3001",
        "start:vite_cypress-preview": "vite preview --port=3001",
        "syntax_check": "tsc --build --force",
        "test:cypress_run-chromium": "cypress run --browser=chromium --e2e",
        "test:cypress_open-chromium": "cypress open --browser=chromium --e2e",
        "test:cypress_run-ff": "cypress run --browser=firefox --e2e",
        "test:cypress_open-ff": "cypress open --browser=firefox --e2e",
        "slintpad:prepublish": "npm run clean && npm run build:wasm_lsp-release && npm run build:wasm_interpreter-release",
        "postinstall": "monaco-treemending"
    },
    "keywords": [],
    "author": "",
    "devDependencies": {
        "@lumino/default-theme": "^2.1.1",
        "@lumino/dragdrop": "^2.1.1",
        "@lumino/widgets": "^2.1.1",
        "@types/vscode": "1.83.1",
        "@typescript-eslint/eslint-plugin": "^6.7.0",
        "@typescript-eslint/parser": "^6.7.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.32.0",
        "monaco-editor": "0.44.0",
        "monaco-languageclient": "6.6.1",
        "@codingame/monaco-vscode-configuration-service-override": "1.83.3",
        "@codingame/monaco-vscode-editor-service-override": "1.83.3",
        "@codingame/monaco-vscode-languages-service-override": "1.83.3",
        "@codingame/monaco-vscode-model-service-override": "1.83.3",
        "@codingame/monaco-vscode-quickaccess-service-override": "1.83.3",
        "@codingame/monaco-vscode-snippets-service-override": "1.83.3",
        "@codingame/monaco-vscode-storage-service-override": "1.83.3",
        "path-browserify": "^1.0.1",
        "rimraf": "^5.0.0",
        "typescript": "^5.0.4",
        "vite": "^4.3.3",
        "vscode": "npm:@codingame/monaco-vscode-api@1.83.3",
        "vscode-languageserver": "8.1.0",
        "vscode-languageserver-protocol": "3.17.3"
    }
}
