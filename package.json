    "lint": "pnpm --stream -r lint",
    "lint:fix": "pnpm --stream -r lint:fix",
    "type-check": "pnpm --stream -r type-check"
  },
  "packageManager": "pnpm@10.10.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "esbuild",
      "sharp"
    ]
  }
}
