import {Button, AboutSlint} from "std-widgets.slint";

export component AppWindow inherits Window {
    preferred-width: 600px;
    preferred-height: 300px;
    property <int> count;

    VerticalLayout {
        AboutSlint { }
        Button {
            text: "Press me";
            clicked => { count += 1; }
        }
        Text { text: count; }
        Rectangle {  }
    }
}
